

server {

        listen  29090 ssl http2;

        include /etc/nginx/ssl_snippet.conf;

        # Process Exporter
        location = /process {
                proxy_pass http://{{ install_name }}_process_exporter:9256/metrics;
        }

        # Cadvisor 
        location = /cadvisor {
                proxy_pass http://{{ install_name }}_cadvisor:8080/metrics;
        }

}